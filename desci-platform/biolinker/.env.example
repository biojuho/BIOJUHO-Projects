# BioLinker Environment Variables
# Copy this file to .env and configure

# OpenAI (LLM 분석 활성화)
OPENAI_API_KEY=your_openai_api_key_here

# CORS
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:5174

# Pinata IPFS (선택)
PINATA_JWT=your_pinata_jwt_here
# OR
PINATA_API_KEY=your_pinata_api_key
PINATA_API_SECRET=your_pinata_secret

# Web3 / Ethereum (선택)
WEB3_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_KEY
DSCI_CONTRACT_ADDRESS=0x...your_contract_address
DISTRIBUTOR_PRIVATE_KEY=your_private_key_for_rewards


GOOGLE_API_KEY=your_google_api_key_here

# 결제 - 카카오페이
KAKAO_PAY_SECRET_KEY=your_kakao_pay_secret_key
KAKAO_PAY_CID=TC0ONETIME

# 결제 - 토스페이먼츠
TOSS_CLIENT_KEY=your_toss_client_key
TOSS_SECRET_KEY=your_toss_secret_key

# 결제 콜백 URL
PAYMENT_BASE_URL=http://localhost:5173
PAYMENT_API_BASE_URL=http://localhost:8001
